.weui_grids {
  position: relative;
  overflow: hidden;
  background-color:#fff;
  margin-top:10px;
  width:100%;
  height:auto;
}
.weui_grids:before {
  content: '';
  position: absolute;
  box-sizing: border-box;
  width: 200%;
  height: 200%;
  left: 0;
  top: 0;
  border: 1px solid #d9d9d9;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scale(.5);
  transform: scale(.5);
}
.weui_grid {
  position: relative;
  float: left;
  padding: 10px 10px;
  width: 25%;
  box-sizing: border-box;
}
.weui_grid_icon {
  width: 40px;
  height: 40px;
  margin: 0 auto;
  border-radius: 50%;
}
.weui_grid_icon image {
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.weui_grid_icon + .weui_grid_label {
  margin-top: 5px;
}

.weui_grid_label {
  display: block;
  text-align: center;
  color: #000;
  font-size: 14px;
}
